{
  "name": "gitcut",
  "version": "1.15.0",
  "description": "Shortcuts for git",
  "main": "index.js",
  "repository": "https://github.com/dingff/gitcut",
  "bin": {
    "gt": "./bin/index"
  },
  "scripts": {
    "release": "release-it --only-version",
    "prepare": "husky"
  },
  "keywords": ["git", "npm", "node", "shortcut", "submit", "query"],
  "author": "dingff",
  "license": "MIT",
  "dependencies": {
    "inquirer": "8.2.6",
    "ora": "5.4.1",
    "picocolors": "^1.1.1",
    "string-width": "4.2.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@release-it/conventional-changelog": "^10.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "release-it": "^18.1.2"
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "lint-staged": {
    "*": ["biome check --write --no-errors-on-unmatched --files-ignore-unknown=true"]
  }
}
